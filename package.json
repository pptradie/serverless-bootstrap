{
  "dependencies": {
    "mysql2": "^2.3.3",
    "sequelize": "^6.23.1",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "jest": "^29.0.2"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
